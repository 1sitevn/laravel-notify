{
    "name": "onesite/notify",
    "description": "Send notification v1.7.",
    "license": "MIT",
    "authors": [
        {
            "name": "1Site.vn",
            "email": "hotro@1site.vn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^6.0|^7.0",
        "illuminate/container": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/encryption": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "hashids/hashids": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OneSite\\Notify\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneSite\\Notify\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "OneSite\\Notify\\Providers\\AppServiceProvider",
                "OneSite\\Notify\\Providers\\EventServiceProvider"
            ]
        }
    }
}
